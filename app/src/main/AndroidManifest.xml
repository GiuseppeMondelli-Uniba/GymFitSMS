<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gymfit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PasswordRecovery" />
        <activity android:name=".system.onboard.FirstOnboardActivity" />
        <activity android:name=".gym.profile.GymProfile"
            android:parentActivityName=".user.signin.Login"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".user.signup.SignUp" />
        <activity android:name=".user.signin.Login" />
        <activity android:name=".system.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>